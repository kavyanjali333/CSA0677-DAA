# Master Theorem
def master_theorem(a, b, k):
    return f"T(n) = O(n^{k})"

# Substitution Method
def substitution_method(n):
    return f"T(n) = O(nlogn)"

# Iteration Method
def iteration_method(n):
    if n == 0:
        return 1
    else:
        return 2 * iteration_method(n-1)

# Example Recurrence Relation
n = 5
print(f"Using Iteration Method: T({n}) = {iteration_method(n)}")
