def find_palindromic_string(words):
    for word in words:
        if word == word[::-1]:
            return word
    return ""

# Example 1
words1 = ["abc", "car", "ada", "racecar", "cool"]
output1 = find_palindromic_string(words1)
print(output1)  # Output: "ada"

# Example 2
words2 = ["notapalindrome", "racecar"]
output2 = find_palindromic_string(words2)
print(output2)  # Output: "racecar"
